    "upgrade": "yarn add cdk8s@latest && yarn add cdk8s-cli@latest -D"
  },
  "dependencies": {
    "cdk8s": "^0.33.0",
    "constructs": "^3.1.3",
    "@pennlabs/kittyhawk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.3",
    "cdk8s-cli": "^0.33.0",
    "typescript": "^4.0.3"
  }
}
