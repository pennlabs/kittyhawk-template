{
  "name": "kittyhawk-template",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "yarn run cdk8s import",
    "synth": "yarn run cdk8s synth",
    "compile": "tsc",
