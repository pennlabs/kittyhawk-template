{
  "name": "kittyhawk-template",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "npx cdk8s import",
    "synth": "npx cdk8s synth",
    "compile": "tsc",
    "watch": "tsc -w",
    "build": "npm run compile && npm run synth",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next"
  },
  "dependencies": {
    "cdk8s": "^0.33.0",
    "@pennlabs/kittyhawk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.3",
    "cdk8s-cli": "^0.33.0",
    "typescript": "^4.0.3"
  }
}
